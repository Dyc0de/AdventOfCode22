evaluateRoundPart1(opponent, me) := 6 {
	isWin(opponent, me)
} else := 3 {
	isDraw(opponent, me)
} else := 0

pointFor(me) := 1 {
	isRock(me)
} else := 2 {
	isPaper(me)
} else := 3 {
	isScissors(me)
}

rounds := [round | round := split(split(challengeInput, "\n")[_], " ")]

round_res := [r | r := evaluateRoundPart1(rounds[i][0], rounds[i][1]) + pointFor(rounds[i][1])]

res := sum(round_res)
